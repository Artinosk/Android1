<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeActivity">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="match_parent"
            android:layout_height="961dp"
            android:scaleX="1.5"
            android:scaleY="1.5"
            app:srcCompat="@drawable/sc5_background" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="match_parent"
            android:layout_height="161dp"
            android:layout_gravity="bottom"
            android:scaleX="1.5"
            android:scaleY="1.5"
            app:srcCompat="@drawable/sc5_bottom_panel" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:translationX="99px"
            android:translationY="500px"
            app:srcCompat="@drawable/sc5_popular" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:scaleX="1.3"
            android:scaleY="1.3"
            android:translationY="50px"
            app:srcCompat="@drawable/sc5_text_main" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="37dp"
            android:layout_height="36dp"
            android:background="@android:color/transparent"
            android:translationX="30px"
            android:translationY="60px"
            app:srcCompat="@drawable/sc5_3_lines"
            tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/imageView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@android:drawable/screen_background_light_transparent"
            android:translationX="900px"
            android:translationY="60px"
            app:srcCompat="@drawable/sc5_basket_up_right"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <ImageButton
            android:id="@+id/all_button1"
            android:layout_width="39dp"
            android:layout_height="28dp"
            android:background="#00FFFFFF"
            android:scaleX="1.5"
            android:scaleY="1.5"
            android:translationX="900px"
            android:translationY="500px"
            app:srcCompat="@drawable/sc5_blue_button_all"
            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="211dp"
            android:orientation="horizontal"
            android:translationY="800px">

            <FrameLayout
                android:layout_width="174dp"
                android:layout_height="205dp"
                android:translationX="30px">

                <ImageButton
                    android:id="@+id/sniker_button1"
                    android:layout_width="183dp"
                    android:layout_height="214dp"
                    android:background="#00FFFFFF"
                    android:scaleX="1"
                    android:scaleY="1"
                    android:translationX="0px"
                    app:srcCompat="@drawable/sc5_pannel_with_stniker"
                    tools:ignore="SpeakableTextPresentCheck" />

                <ImageButton
                    android:id="@+id/sniker_button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:background="#00FFFFFF"
                    android:translationY="50px"
                    app:srcCompat="@drawable/sc5_sniker"
                    tools:ignore="SpeakableTextPresentCheck" />

                <ImageButton
                    android:id="@+id/heart_button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#00FFFFFF"
                    android:scaleX="1.1"
                    android:scaleY="1.1"
                    android:translationX="50px"
                    android:translationY="55px"
                    app:srcCompat="@drawable/sc5_red_heart"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                <ImageButton
                    android:id="@+id/basket_button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|right"
                    android:background="#00FFFFFF"
                    android:translationX="-15px"
                    android:translationY="-20px"
                    app:srcCompat="@drawable/sc5_plus"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

            </FrameLayout>

            <FrameLayout
                android:layout_width="174dp"
                android:layout_height="205dp"
                android:translationX="130px">

                <ImageButton
                    android:id="@+id/sniker_button6"
                    android:layout_width="183dp"
                    android:layout_height="214dp"
                    android:background="#00FFFFFF"
                    android:scaleX="1"
                    android:scaleY="1"
                    android:translationX="0px"
                    app:srcCompat="@drawable/sc5_pannel_with_stniker"
                    tools:ignore="SpeakableTextPresentCheck" />

                <ImageButton
                    android:id="@+id/sniker_button7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:background="#00FFFFFF"
                    android:translationY="50px"
                    app:srcCompat="@drawable/sc5_sniker"
                    tools:ignore="SpeakableTextPresentCheck" />

                <ImageButton
                    android:id="@+id/heart_button4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#00FFFFFF"
                    android:scaleX="1.1"
                    android:scaleY="1.1"
                    android:translationX="50px"
                    android:translationY="55px"
                    app:srcCompat="@drawable/sc5_white_heart"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                <ImageButton
                    android:id="@+id/basket_button2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|right"
                    android:background="#00FFFFFF"
                    android:translationX="-10px"
                    android:translationY="-10px"
                    app:srcCompat="@drawable/sc5_plus"
                    tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
            </FrameLayout>

        </LinearLayout>

        <ImageButton
            android:id="@+id/all_button2"
            android:layout_width="39dp"
            android:layout_height="28dp"
            android:background="#00FFFFFF"
            android:scaleX="1.5"
            android:scaleY="1.5"
            android:translationX="900px"
            android:translationY="1400px"
            app:srcCompat="@drawable/sc5_blue_button_all"
            tools:ignore="PxUsage,TouchTargetSizeCheck,SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/imageButton13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:background="#00ffffff"
            android:scaleX="1.2"
            android:scaleY="1.2"
            android:translationY="1600px"
            app:srcCompat="@drawable/sc5_advertisment"
            tools:ignore="SpeakableTextPresentCheck" />

        <HorizontalScrollView
            android:layout_width="match_parent"
            android:layout_height="57dp"
            android:translationY="230dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/imageButton10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#00ffffff"
                    app:srcCompat="@drawable/sc5_all_button" />

                <ImageButton
                    android:id="@+id/imageButton11"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#00ffffff"
                    app:srcCompat="@drawable/sc5_outdoor_button" />

                <ImageButton
                    android:id="@+id/imageButton12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#00ffffff"
                    app:srcCompat="@drawable/sc5_tennis_button" />

                <ImageButton
                    android:id="@+id/imageButton14"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#00ffffff"
                    app:srcCompat="@drawable/sc5_running_button" />
            </LinearLayout>
        </HorizontalScrollView>

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>